fd2pragma: T:fd2pragma.o
	slink SMALLDATA SMALLCODE NOICONS STRIPDEBUG LIB LIB:sc.lib FROM LIB:c.o $? TO $@

T:fd2pragma.o: fd2pragma.c
	sc RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS CHECKABORT NOSTACKEXTEND OPTIMIZERSIZE\
OPTIMIZE OPTSCHEDULE NOICONS OPTINLOCAL MEMSIZE=HUGE STRINGSECTION=NEAR CODE=FAR $? OBJNAME=$@\
DEFINE=FD2PRAGMA_READARGS DEFINE=FD2PRAGMA_AMIGA

fd2pragmaD: T:fd2pragmaD.o
	slink SMALLDATA SMALLCODE NOICONS LIB LIB:sc.lib FROM LIB:c.o $? TO $@

T:fd2pragmaD.o: fd2pragma.c
	sc RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS CHECKABORT NOSTACKEXTEND NOOPT\
NOICONS MEMSIZE=HUGE STRINGSECTION=NEAR CODE=FAR $? OBJNAME=$@ DEBUG=LINE DEFINE=DEBUG\
DEFINE=FD2PRAGMA_READARGS DEFINE=FD2PRAGMA_AMIGA #DEFINE=DEBUG_OLD

fd2pragmaU: T:fd2pragmaU.o
	slink SMALLDATA SMALLCODE NOICONS STRIPDEBUG LIB LIB:sc.lib FROM LIB:c.o $? TO $@

T:fd2pragmaU.o: fd2pragma.c
	sc RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS CHECKABORT NOSTACKEXTEND OPTIMIZERSIZE\
OPTIMIZE OPTSCHEDULE NOICONS OPTINLOCAL MEMSIZE=HUGE STRINGSECTION=NEAR CODE=FAR $? OBJNAME=$@\
DEFINE=FD2PRAGMA_AMIGA

fd2pragmaGCC: fd2pragma.c
	gcc -Wall -O3 -DFD2PRAGMA_READARGS -DFD2PRAGMA_AMIGA -o$@ fd2pragma.c

fd2pragmaNO: T:fd2pragmaNO.o
	slink SMALLDATA SMALLCODE NOICONS STRIPDEBUG LIB LIB:sc.lib FROM LIB:c.o $? TO $@

T:fd2pragmaNO.o: fd2pragma.c
	sc RESOPT PARAMETERS=REGISTERS NOSTACKCHECK STRINGMERGE UNSIGNEDCHARS CHECKABORT NOSTACKEXTEND NOICONS\
MEMSIZE=HUGE STRINGSECTION=NEAR CODE=FAR $? OBJNAME=$@ DEFINE=FD2PRAGMA_READARGS DEFINE=FD2PRAGMA_AMIGA

fd2pragmaVBCC: fd2pragma.c
	vc -O2047 -DFD2PRAGMA_READARGS -DFD2PRAGMA_AMIGA -o=$@ fd2pragma.c
#	vc -O8191 -DFD2PRAGMA_READARGS -DFD2PRAGMA_AMIGA -o=$@ fd2pragma.c

fd2pragmaMOS: fd2pragma.c
	vc +morphos -O2 -DFD2PRAGMA_READARGS -DFD2PRAGMA_AMIGA -o=$@ fd2pragma.c

